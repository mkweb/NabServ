body {
	font-family: 'Ubuntu', sans-serif;
}
h2 {
	font-weight: normal;
	margin: 0;
	margin-left: 4px;
	float: left;
}
#content h3 {
	margin-bottom: 2px;
	margin-left: 4px;
	float: left;
}
#content h4 {
	margin: 0;
	margin-top: 20px;
}
hr {
	border: none;
	border-bottom: 1px solid #000;
	clear: both;
}
a {
	text-decoration: none;
	color: #000;
}
a:hover {
	text-decoration: underline;
}

a:hover img {
	text-decoration: none;
}

#content table {
	border-collapse: collapse;
	border: 1px solid #666;
}
#content table td, #content table th {
	font-size: 8pt;
}
#content table td a {
	color: #000;
}
#content table td {
	padding: 4px;
}
#content table thead tr {
	background-color: #ffd700;
}
#content table thead th {
	text-align: left;
	padding: 4px;
}
#content table tbody tr:hover {
	background-color: #fff3b4;
}

.ui-widget { font-family: Verdana; font-size: 0.7em; }
.ui-widget table { font-size: 8pt; }
.accordion { width: 400px; float: left; }
.ui-accordion-content { height: auto; }

.appimage, .rabitimage {
	display: block;
	text-decoration: none;
	border: none;
	width: 120px;
	height: 120px;
	float: left;
	margin-right: 10px;
}
	.appimage:hover, .rabitimage:hover {
		background-position: 0 120px;
		text-decoration: none;
	}

.img40 {
	width: 40px;
	height: 40px;
}
	.img40:hover {
		background-position: 0 40px;
	}
.img60 {
	width: 60px;
	height: 60px;
}
	.img60:hover {
		background-position: 0 60px;
	}
.img80 {
	width: 80px;
	height: 80px;
}
	.img80:hover {
		background-position: 0 80px;
	}
.img100 {
	width: 100px;
	height: 100px;
}
	.img100:hover {
		background-position: 0 100px;
	}

.availapp_details {
	clear: both;
	display: none;
	margin-top: 4px;
	padding: 10px;
	font-size: 10pt;
	background-color: #FFEBBD;
	position: absolute; 
	top: 0;
	left: 0;
	border: 1px solid #000;
	border-radius: 4px;
	box-shadow: 0 0 10px #000;
}

.flash {
	border: 2px solid #00b806;
	background-color: #ebffec;
	color: #00b806;
	font-weight: bold;
	padding: 4px 8px;
	font-size: 8pt;
	border-radius: 6px;
	position: absolute;
	top: 10px;
	left: 10px;
	z-index: 999;
}

.error {
	border: 2px solid #a30000;
	background-color: #fff0f0;
	color: #a30000;
	font-weight: bold;
	padding: 4px 8px;
	font-size: 8pt;
	border-radius: 6px;
	position: absolute;
	top: 10px;
	left: 10px;
	z-index: 999;
}

.left {
	float: left;
}
.right {
	float: right;
}
#shadow_top {
	width: 893px;
	height: 26px;
	margin: 19px auto 0;
	background-image: url('[BASE_URL]/res/homepage/images/layout/shadow_top.png');
}
#shadow_top .left {
	width: 26px;
	height: 26px;
	background-image: url('[BASE_URL]/res/homepage/images/layout/shadow_top_left.png');
}
#shadow_top .right {
	width: 26px;
	height: 26px;
	background-image: url('[BASE_URL]/res/homepage/images/layout/shadow_top_right.png');
}
#shadow_left {
	width: 22px;
	height: 660px;
	position: absolute;
	top: 10px;
	left: 50%;
	margin-left: -468px;
	background-image: url('[BASE_URL]/res/homepage/images/layout/shadow_left.png');
	
}
#shadow_right {
	width: 22px;
	height: 660px;
	position: absolute;
	top: 10px;
	left: 50%;
	margin-left: 446px;
	background-image: url('[BASE_URL]/res/homepage/images/layout/shadow_right.png');
}
#all {
	width: 893px;
	min-height: 660px;
	margin: 0 auto;
}
#head {
	width: 100%;
	height: 108px;
	background-color: #ffd700;
}
	#logo {
		margin: 20px;
		float: left;
		width: 215px;
		height: 50px;
	}
	#logo a {
		border: none;
		text-decoration: none;
		color: #000;
	}
	#login {
		margin: 20px;
		float: right;
	}
		#login table {
			border: none;
		}
		#login table td {
			padding: 2px;
		}
		#login table tbody tr:hover {
			background-color: transparent;
		}
#navi_top {
	width: 100%;
	height: 7px;
	background-color: #AFAFAF;
	background-image: url('[BASE_URL]/res/homepage/images/layout/navi_top.png');
	background-repeat: repeat-x;
}
#navi {
	width: 100%;
	height: 30px;
	line-height: 30px;
	background-color: #000;
	color: #FFF;
}
	#navi > div {
		margin-left: 10px;
	}
	#navi a {
		margin: 0 20px;
		color: #FFF;
		text-decoration: none;
	}
	#navi a:hover {
		text-decoration: underline;
	}
#navi_bottom {
	width: 100%;
	height: 7px;
	background-color: #AFAFAF;
	background-image: url('[BASE_URL]/res/homepage/images/layout/navi_bottom.png');
	background-repeat: repeat-x;
}
#additional_links {
	background-color: #000;
	position: relative;
	margin-top: -6px;
}
	#additional_links a {
		color: #FFF;
		font-size: 10pt;
		display: block;
		padding: 2px 10px;
	}
#content {
	margin: 10px;
	width: 700px;
	float: left;
}

.input {
	background-color: #FFF;
	text-decoration: none;
	color: #000;
	font-size: 10pt;
	display: block;
	border: 1px solid #000;
	padding: 2px 6px;
	border-radius: 10px;
	box-shadow: 0 0 4px #666;
}
	.inputerror {
		box-shadow: 0 0 4px #F00;
	}
.button {
	background-color: #fff3b4;
	text-decoration: none;
	color: #000;
	font-size: 10pt;
	display: block;
	border: 1px solid #000;
	padding: 2px 6px;
	border-radius: 10px;
	box-shadow: 0 0 4px #666;
	cursor: pointer;
}
.button:hover {
	background-color: #ffd700;
}
.titlebutton {
	float: right;
	margin-top: 16px;
}

#nab_corner {
	width: 150px;
	height: 160px;
	position: fixed;
	left: 100%;
	top: 100%;
	margin-left: -160px;
	margin-top: -170px;
	background-image: url('[BASE_URL]/res/homepage/images/layout/nab_corner.png');
}

.histlinks {
	float: left;
	font-size: 10px;
	vertical-align: bottom;
}

.spacer {
	clear: both;
	width: 100%;
}
